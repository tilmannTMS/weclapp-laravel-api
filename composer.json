{
  "name": "tilmanntms/weclapp-laravel-api",
  "description": "A Laravel package for interacting with the Weclapp API",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "tilmannTMS",
      "email": "your-email@example.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/support": "^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": "^6.0|^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "TilmannTMS\\Weclapp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TilmannTMS\\Weclapp\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TilmannTMS\\Weclapp\\WeclappServiceProvider"
      ],
      "aliases": {
        "Weclapp": "TilmannTMS\\Weclapp\\Facades\\Weclapp"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}