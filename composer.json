{
  "name": "tilmanntms/weclapp-laravel-api",
  "description": "A Laravel package for interacting with the Weclapp API",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tilmann TMS",
      "email": "your-email@example.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "TilmannTMS\\WeclappLaravelApi\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TilmannTMS\\WeclappLaravelApi\\WeclappServiceProvider"
      ],
      "aliases": {
        "Weclapp": "TilmannTMS\\WeclappLaravelApi\\Facades\\Weclapp"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}